datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    String @id() @default(cuid())
  email String @unique()
  tasks Task[]
}

model Task {
  id       String  @id() @default(cuid())
  title    String
  done     Boolean @default(false)
  isPublic Boolean @default(false)
  owner    User    @relation(fields: [ownerId], references: [id])
  ownerId  String
}
